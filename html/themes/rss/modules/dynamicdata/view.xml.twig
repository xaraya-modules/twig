{# @theme/rss/modules/dynamicdata/view.xml.twig #}

{% block user_view %}
{% set dummy = xar_pagetemplate('header') %}
    {% if object %}
        {% set objname = object.name %}
        {% set dummy = xar_varcache('XML', 'itemname', objname) %}
        {{ xar_data_view({object: object}) }}
    {% elseif modid %}
        {% set modinfo = xar_modinfo(modid) %}
        {% set dummy = xar_varcache('XML', 'itemname', modinfo['name'] ~ '_' ~ itemtype) %}
        {{ xar_data_view({module: modid, itemtype: itemtype, numitems: "30", startnum: startnum}) }}
    {% endif %}
{% endblock %}