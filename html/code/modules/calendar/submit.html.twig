{# @calendar/submit.html.twig #}

{% extends '@calendar/base.html.twig' %}

{% block modulespace %}
<form name="cal_submit" method="post" action="{{ xar_moduleurl('calendar','user','submit') }}">

    <p>
        Title : <input type="text" name="title" id="title" value="{{ event_title }}"/>
    </p>
    <p>
        Description : <br />
        <textarea name="description" id="description">{{ event_description }}</textarea>
    </p>

    </form>
{% endblock %}