{# @base/properties/showhidden-fileupload.html.twig #}

{% block showhidden_fileupload %}
<input type="hidden" name="{{ name }}" id="{{ id }}_hidden" value="{{ value }}"/>
    <input type="hidden" name="MAX_FILE_SIZE" value="{{ maxsize }}"/>
    <input type="hidden" name="{{ name }}" size="{{ size }}" id="{{ id }}" style="display: none"/>
{% endblock %}