{# @dynamicdata/includes/user-mod-head.html.twig #}

{% block user_mod_head %}
<div class="xar-mod-head">
        <span class="xar-mod-title">
            {% if modtitle is defined %}
                {{ modtitle }}
            {% elseif xar_modulevar('dynamicdata', 'use_module_alias') %}
                {{ xar_modulevar('dynamicdata', 'module_alias_name') }}
            {% else %}
                DynamicData Module
            {% endif %}
        </span>
    </div>
{% endblock %}