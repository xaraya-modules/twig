{# @dynamicdata/includes/user-mod-head.html.twig #}

{% block user_mod_head %}
<div class="xar-mod-head">
        <span class="xar-mod-title">
            {% if xar_modvar('dynamicdata', 'use_module_alias') %}
                {{ xar_var({scope: "module", module: "dynamicdata", name: "module_alias_name"}) }}
            {% else %}
                DynamicData Module
            {% endif %}
        </span>
    </div>
{% endblock %}