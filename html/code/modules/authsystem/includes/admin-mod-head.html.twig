{# @authsystem/includes/admin-mod-head.html.twig #}

{% block admin_mod_head %}
<div class="xar-mod-head">
        <span class="xar-mod-title">
            {% if xar_modvar('authsystem', 'use_module_alias') %}
                {{ xar_var({scope: "module", module: "authsystem", name: "module_alias_name"}) }}
            {% else %}
                Authsystem Administration
            {% endif %}
        </span>
        {{ include('@authsystem/includes/admin-menu.html.twig') }}
    </div>
{% endblock %}