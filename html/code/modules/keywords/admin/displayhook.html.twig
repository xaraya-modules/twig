{# @keywords/admin/displayhook.html.twig #}

{% block displayhook %}
{#   License: GPL http://www.gnu.org/copyleft/gpl.html  #}
{% if keywords %}
    <div class="xar-keywords-list">
        {% if showlabel %}
        Keywords:&#160;
        {% endif %}
        {% for loop_key1, loop_item1 in keywords %}{% set loop_index1 = loop.index %}
            <a href="{{ xar_moduleurl('keywords', 'admin', 'view', {'keyword': loop_item1}) }}">{{ loop_item1 }}</a>
            {% if loop_index1 < keywords|length %},&#160;{% endif %}
        {% endfor %}
    </div>
{% endif %}
{% endblock %}